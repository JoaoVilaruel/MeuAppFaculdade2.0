<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Projetoduda">
        <activity
            android:name=".TelaFinanceiro"
            android:exported="false"
            android:label="@string/title_activity_tela_financeiro"
            android:theme="@style/Theme.Projetoduda" />
        <activity
            android:name=".BottomBar"
            android:exported="false"
            android:label="@string/title_activity_bottom_bar"
            android:theme="@style/Theme.Projetoduda" />
        <activity
            android:name=".TelaPerfil"
            android:exported="false"
            android:label="@string/title_activity_tela_perfil"
            android:theme="@style/Theme.Projetoduda" />
        <activity
            android:name=".TelaHome"
            android:exported="false"
            android:label="@string/title_activity_tela_home"
            android:theme="@style/Theme.Projetoduda" />
        <activity
            android:name=".TelaCurso"
            android:exported="false"
            android:label="@string/title_activity_tela_curso"
            android:theme="@style/Theme.Projetoduda" />
        <activity
            android:name=".AppNavigation"
            android:exported="false"
            android:label="@string/title_activity_app_navigation"
            android:theme="@style/Theme.Projetoduda" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Projetoduda">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>